{
  "name": "proto-store",
  "version": "0.0.1",
  "description": "A utility module to establish a convenient local storage facility for prototype development.",
  "main": "proto-store.js",
  "type": "module",
  "scripts": {
    "test": "node --test",
    "watch": "node --test --watch",
    "coverage": "c8 node --test",
    "coverage:ui": "http-server ./coverage -o lcov-report/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TracyGJG/proto-store.git"
  },
  "keywords": [
    "Prototyping",
    "Storage"
  ],
  "author": "TGJ Gilmore",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/TracyGJG/proto-store/issues"
  },
  "homepage": "https://github.com/TracyGJG/proto-store/#readme",
  "devDependencies": {
    "c8": "^10.1.3",
    "http-server": "^14.1.1"
  },
  "c8": {
    "exclude": [
      "**/*.test.*"
    ],
    "reporter": [
      "lcov"
    ]
  }
}
